LIMIT 1;</sql><sql name="most_revenue*">SELECT
    gender,
    CASE
        WHEN age &lt;= 45 THEN '45 or younger'
        WHEN age &gt; 45 THEN 'over 45'
        ELSE 'nothing'
    END AS age_group,
    SUM(price) AS total_revenue
FROM
    personen
GROUP BY
    gender, age_group
ORDER BY
    total_revenue DESC;</sql><sql name="avg_basket_value*">SELECT
	AVG(price)*AVG(quantity) AS average_basket_value 
FROM
	personen
</sql><sql name="old_category*">SELECT
    category,
    COUNT(*) AS anzahl
FROM
    personen
WHERE
    age &gt; 45
GROUP BY
    category
ORDER BY
    anzahl DESC
LIMIT 1;</sql><sql name="shoppingcenter_highest_revenue*">SELECT
	shopping_mall,
	sum(price * quantity) AS Revenue
FROM
	personen
GROUP BY
	shopping_mall
ORDER BY
	Revenue DESC
LIMIT 3;</sql><sql name="Avg_purchase_number*">SELECT
    AVG(purchase_count) AS average_purchases_per_customer
FROM (
    SELECT
        customer_id,
        COUNT(invoice_no) AS purchase_count
    FROM
        personen
    GROUP BY
        customer_id
);</sql><current_tab id="9"/></tab_sql></sqlb_project>
