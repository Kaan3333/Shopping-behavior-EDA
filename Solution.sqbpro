    -- Numerische Durchschnittswerte
    (SELECT AVG(age) FROM personen) AS avg_age,
    (SELECT AVG(quantity) FROM personen) AS avg_quantity,
    (SELECT AVG(price) FROM personen) AS avg_price,

    -- Geschlechtsverteilung
    (SELECT COUNT(*) FROM personen WHERE gender = 'Female') AS female_count,
    (SELECT COUNT(*) FROM personen WHERE gender = 'Male') AS male_count,

    -- Verteilung der Zahlungsmethoden
    (SELECT COUNT(*) FROM personen WHERE payment_method = 'Credit Card') AS credit_card_count,
    (SELECT COUNT(*) FROM personen WHERE payment_method = 'Cash') AS cash_count,
    (SELECT COUNT(*) FROM personen WHERE payment_method = 'Debit Card') AS debit_card_count,

    -- Verteilung der Produktkategorien
    (SELECT COUNT(*) FROM personen WHERE category = 'Clothing') AS clothing_count,
    (SELECT COUNT(*) FROM personen WHERE category = 'Technology') AS technology_count,
    (SELECT COUNT(*) FROM personen WHERE category = 'Food &amp; Beverage') AS food_beverage_count,
    (SELECT COUNT(*) FROM personen WHERE category = 'Shoes') AS shoes_count,
    (SELECT COUNT(*) FROM personen WHERE category = 'Books') AS books_count,
    (SELECT COUNT(*) FROM personen WHERE category = 'Cosmetics') AS cosmetics_count,
    (SELECT COUNT(*) FROM personen WHERE category = 'Toys') AS toys_count,
    (SELECT COUNT(*) FROM personen WHERE category = 'Souvenir') AS souvenir_count
FROM
    personen
LIMIT 1;</sql><sql name="avg_male*">SELECT
